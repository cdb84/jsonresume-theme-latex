{
  "name": "jsonresume-theme-latex",
  "version": "0.1.1",
  "description": "A LaTeX theme/output format for JSON resumes.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "check-style": "prettier -l lib/**/*.ts",
    "lint": "tslint -p tsconfig.json",
    "prepare": "run-s build",
    "run-tests": "mocha -r ts-node/register lib/**/*.spec.ts",
    "test": "run-s check-style lint run-tests"
  },
  "repository": "https://github.com/ianprime0509/jsonresume-theme-latex",
  "author": {
    "name": "Ian Johnson",
    "email": "ianprime0509@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@ianprime0509/jsonresume-schema": "0.0.3",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.1",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "moment": "^2.22.2"
  }
}
