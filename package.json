{
  "name": "jsonresume-theme-latex",
  "version": "0.1.1",
  "description": "A LaTeX theme/output format for JSON resumes.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "check-style": "prettier -l lib/**/*.ts",
    "lint": "tslint -p tsconfig.json",
    "prepare": "run-s build",
    "run-tests": "mocha -r ts-node/register lib/**/*.spec.ts",
    "test": "run-s check-style lint run-tests"
  },
  "repository": "https://github.com/ianprime0509/jsonresume-theme-latex",
  "author": {
    "name": "Ian Johnson",
    "email": "ianprime0509@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@ianprime0509/jsonresume-schema": "0.0.4",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "moment": "^2.27.0"
  }
}
